{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <div class="container-fluid">
                <div class="pull-left">
                    <h1>{{ heading_title }}</h1>
                </div>
                <div class="pull-right">
                    {#                    <a href="{{ insert }}" data-toggle="tooltip" title="{{ button_insert }}"#}
                    {#                       class="btn btn-primary"><i class="fa fa-plus"></i></a>#}

                    <button type="button" data-toggle="tooltip" title="Start" class="btn btn-success start_upload">
                        Start
                    </button>
                </div>
            </div>
            {% if breadcrumps %}
                <ul class="breadcrumb">
                    {% for breadcrumb in breadcrumbs %}
                        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                    {% endfor %}
                </ul>
            {% endif %}
        </div>
    </div>
    <div class="container-fluid">
        {% if success %}
            <div class="alert alert-success"><i class="fa fa-check-circle"></i> {{ success }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endif %}
        <div class="panel panel-default">
            <div class="panel-body">
                <form action="{{ delete }}" method="post" enctype="multipart/form-data" id="form">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="table-responsive">
                                <table class="table table-bordered table-hover">
                                    <thead>
                                    <tr>
                                        <td style="width: 1px;" class="text-center"></td>
                                        <td class="text-left" style="color: #1E91CF;">{{ text_name }}</td>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    {% if supplers %}
                                        {% for suppler in supplers %}
                                            <tr>
                                                <td style="text-align: center;">{% if suppler.selected %}
                                                        <input type="checkbox"
                                                               name="selected[]"
                                                               class="supplers_checkbox"
                                                               value="{{ suppler.id }}"
                                                               data-suppler_id="{{ suppler.suppler_id }}"
                                                               checked="checked"
                                                        />
                                                    {% else %}
                                                        <input type="checkbox"
                                                               name="selected[]"
                                                               class="supplers_checkbox"
                                                               data-suppler_id="{{ suppler.suppler_id }}"
                                                               value="{{ suppler.id }}"
                                                        />
                                                    {% endif %}</td>
                                                <td class="text-left">{{ suppler.name }}</td>
                                            </tr>
                                        {% endfor %}
                                    {% else %}
                                        <tr>
                                            <td class="text-center" colspan="4">{{ text_no_results }}</td>
                                        </tr>
                                    {% endif %}
                                    </tbody>
                                </table>
                                <input type="hidden" name="token" value="{{ token }}"/>
                            </div>
                        </div>
                        <div class="col-md-9">
                            <div class="text-center ajax_upload">
                                <div class="alert alert-info" role="alert">
                                    {{ text_xml_upload_all_start }}
                                </div>
{#                                <a data-toggle="tooltip" class="btn btn-primary start_upload"#}
{#                                   type="button"><i>Start</i></a>#}

                            </div>
                            <div class="list_end"></div>
                        </div>
                    </div>
                </form>
                <div class="row">
                    <div class="col-sm-6 text-left">{{ pagination }}</div>
                </div>
            </div>
        </div>
    </div>
</div>
{{ footer }}